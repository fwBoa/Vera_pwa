{
  "name": "vera-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/vera-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/vera-app",
        "browser": "apps/vera-app/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/vera-app/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/vera-app/public"
          }
        ],
        "styles": [
          "apps/vera-app/src/styles.css"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/vera-app/src/environments/environment.ts",
              "with": "apps/vera-app/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "vera-app:build:production"
        },
        "development": {
          "buildTarget": "vera-app:build:development",
          "proxyConfig": "apps/vera-app/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "vera-app:build"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "vera-app:build",
        "staticFilePath": "dist/apps/vera-app/browser",
        "spa": true
      }
    }
  }
}